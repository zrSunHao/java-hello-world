<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry
		location="C:\Users\x\.m2\repository\mysql\mysql-connector-java\8.0.15\mysql-connector-java-8.0.15.jar" />

	<context id="MysqlTables" targetRuntime="MyBatis3">
		<!--去除注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/java-helloworld?serverTimezone=GMT" userId="root"
			password="root">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.sun.helloworld.entity"
			targetProject="D:\git\eclipse-workspace\.metadata\HelloWorld\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- 想要逆向生成的表在这里进行配置 -->
		<table schema="gp" tableName="test_user" domainObjectName="test_user">
			<property name="useActualColumnNames" value="false" />
			
		</table>


	</context>
</generatorConfiguration>
